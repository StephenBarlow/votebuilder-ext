{
  "manifest_version": 2,
  "name": "VoteBuilder NC Shortcuts",
  "description": "Provides hotkeys for VoteBuilder. Probably only useful for volunteers with Hillary for North Carolina.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["https://www.votebuilder.com/MyListScriptGrid.aspx*"],
      "js": ["jquery-3.1.0.min.js", "gridview.js"],
      "css": ["gridview.css"]
    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "icons": {
    "48": "icon-small.png",
    "128": "icon.png"
  }
}
